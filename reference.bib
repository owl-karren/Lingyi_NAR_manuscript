	Journal = {Scientific Reports},
	Number = {1},
	Pages = {16252},
	Title = {propr: An R-package for Identifying Proportionally Abundant Features Using Compositional Data Analysis},
	Ty = {JOUR},
	Url = {https://doi.org/10.1038/s41598-017-16520-0},
	Volume = {7},
	Year = {2017},
	Bdsk-Url-1 = {https://doi.org/10.1038/s41598-017-16520-0}}

