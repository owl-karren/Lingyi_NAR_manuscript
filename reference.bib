@article{Solonenko2013,
	Abstract = {Microbes drive the biogeochemistry that fuels the planet. Microbial viruses modulate their hosts directly through mortality and horizontal gene transfer, and indirectly by re-programming host metabolisms during infection. However, our ability to study these virus-host interactions is limited by methods that are low-throughput and heavily reliant upon the subset of organisms that are in culture. One way forward are culture-independent metagenomic approaches, but these novel methods are rarely rigorously tested, especially for studies of environmental viruses, air microbiomes, extreme environment microbiology and other areas with constrained sample amounts. Here we perform replicated experiments to evaluate Roche 454, Illumina HiSeq, and Ion Torrent PGM sequencing and library preparation protocols on virus metagenomes generated from as little as 10pg of DNA.},
	Author = {Solonenko, Sergei A. and Ignacio-Espinoza, J. C{\'e}sar and Alberti, Adriana and Cruaud, Corinne and Hallam, Steven and Konstantinidis, Kostas and Tyson, Gene and Wincker, Patrick and Sullivan, Matthew B.},
