	Doi = {10.1128/mSphere.00327-17},
	Editor = {Krajmalnik-Brown, Rosa},
	Elocation-Id = {e00327-17},
	Eprint = {https://msphere.asm.org/content/2/5/e00327-17.full.pdf},
	Journal = {mSphere},
	Number = {5},
	Publisher = {American Society for Microbiology Journals},
	Title = {The Gut Microbiota of Healthy Aged Chinese Is Similar to That of the Healthy Young},
	Url = {https://msphere.asm.org/content/2/5/e00327-17},
	Volume = {2},
	Year = {2017},
	Bdsk-Url-1 = {https://msphere.asm.org/content/2/5/e00327-17},
