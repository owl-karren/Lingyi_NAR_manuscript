	Isbn = {0169-7439},
	Journal = {Chemometrics and Intelligent Laboratory Systems},
	Keywords = {Censored data; Nondetects; Zeros; Compositional data; Imputation; Log-ratio analysis},
	Pages = {85--96},
	Title = {zCompositions ---R package for multivariate imputation of left-censored data under a compositional approach},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0169743915000490},
	Volume = {143},
