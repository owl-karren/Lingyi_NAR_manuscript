	Elocation-Id = {559831},
	Eprint = {https://www.biorxiv.org/content/early/2019/02/25/559831.full.pdf},
	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Consistent and correctable bias in metagenomic sequencing measurements},
	Url = {https://www.biorxiv.org/content/early/2019/02/25/559831},
	Year = {2019},
