	Journal = {Annals of Epidemiology},
	Keywords = {Compositional data; 16S rRNA gene sequencing; Microbiome; Multivariate analysis},
	Number = {5},
	Pages = {322--329},
	Title = {It's all relative: analyzing microbiome data as compositions},
